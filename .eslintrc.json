{
  "plugins": [
    "import",
    "json-format"
  ],
  "env": {
    "es6": true,
    "applescript": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "warn"
  },
  "noInlineConfig": true,
  "settings": {
    "import/resolver": "webpack",
    "json/json-with-comments-files": []
  },
  "overrides": [
    {
      "files": [
        "webpack.config.js"
      ],
      "env": {
        "es6": false,
        "applescript": false,
        "commonjs": true
      },
      "rules": {
        "import/no-extraneous-dependencies": "off"
      }
    }
  ]
}
