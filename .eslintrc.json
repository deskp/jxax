{
    "env": {
        "es6": true,
        "applescript": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "settings": {
        "import/resolver": "webpack"
    },
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "warn"
    },
    "noInlineConfig": true,
    "overrides": [
        {
            "files": [ "webpack.config.js" ],
            "env": {
                "es6": false,
                "applescript": false,
                "commonjs": true
            },
            "rules": {
                "import/no-extraneous-dependencies": "off"
            }
        }
    ]
}
