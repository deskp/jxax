{
    "env": {
        "es6": true,
        "commonjs": true,
        "applescript": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "settings": {
        "import/resolver": "webpack"
    },
    "rules": {
        "no-console": "off"
    }
}
